{
  "name": "zillow-scraper",
  "version": "1.0.0",
  "main": "zillow.js",
  "scripts": {
    "postinstall": "npx puppeteer browsers install chrome",
    "start": "node zillow.js",
    "dev": "node zillow.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:proxy": "node test-proxy.js",
    "scrape:all": "node scripts/run-all-regions.js",
    "scrape:region": "node scripts/run-region.js",
    "list-regions": "node scripts/list-regions.js",
    "detect:all": "node detectSoldListings.js",
    "detect:region": "node detectSoldListings.js",
    "migrate": "node scripts/migrate-database.js",
    "scrape:first-time": "node scripts/run-region-first-time.js",
    "retry:failed": "node scripts/retry-failed-cities.js",
    "detect:cities": "node scripts/detect-specific-cities.js",
    "detect:retry": "node scripts/detect-specific-cities.js",
    "auto:retry": "node scripts/auto-retry-failed.js",
    "delayed:retry": "node scripts/delayed-retry.js",
    "schedule:delayed": "node scripts/schedule-delayed-retry.js",
    "recover:city": "node scripts/recover-city.js",
    "advanced": "node advanced-zillow.js",
    "advanced:scraper": "node advanced-scraper.js",
    "advanced:detection": "node advanced-detection.js",
    "advanced:test": "node -e \"import('./advanced-regional-system.js').then(m => { const system = new m.default(); system.processRegion('east').then(console.log).catch(console.error); })\"",
    "enterprise": "node enterprise-simple.js",
    "enterprise:monitor": "node monitoring-dashboard.js",
    "enterprise:worker": "node queue-worker.js",
    "enterprise:queue": "node queue-manager.js",
    "decodo:test": "node test-decodo.js",
    "decodo:scrape": "node decodo-scraper.js",
    "hybrid:test": "node test-hybrid.js",
    "hybrid:current": "node -e \"import('./hybrid-scraper.js').then(m => new m.default().scrapeCities('current', ['Toronto', 'Windsor']))\"",
    "hybrid:decodo": "node -e \"import('./hybrid-scraper.js').then(m => new m.default().scrapeCities('decodo', ['Toronto', 'Windsor']))\"",
    "hybrid:auto": "node -e \"import('./hybrid-scraper.js').then(m => new m.default().scrapeCities('hybrid', ['Toronto', 'Windsor']))\"",
    "concurrent:test": "node concurrent-decodo-test.js",
    "regional:schedule": "node regional-scheduler.js",
    "enhanced:test": "node -e \"import('./enhanced-hybrid-scraper.js').then(m => new m.default().testSources(['Toronto', 'Vancouver']))\"",
    "enhanced:scrape": "node -e \"import('./enhanced-hybrid-scraper.js').then(m => new m.default().scrapeCities('enhanced', []))\"",
    "advanced:regional": "node advanced-regional-system.js",
    "optimized:regional": "node optimized-regional-system.js",
    "optimized:test": "node -e \"import('./optimized-regional-system.js').then(m => { const system = new m.default(); system.processBatch(0).then(console.log).catch(console.error); })\"",
    "test:now": "node test-optimized-now.js",
    "focused:run": "node enhanced-optimized-system.js",
    "proxy:run": "node focused-proxy-system.js",
    "marketing:test": "node test-marketing-email.js",
    "trigger:kwg": "node trigger-kwg.js"
  },
  "keywords": [
    "zillow",
    "scraper",
    "real-estate",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "description": "Real-time Zillow listing scraper with email notifications and Supabase integration",
  "dependencies": {
    "@supabase/supabase-js": "2.48.1",
    "axios": "1.7.9",
    "bull": "^4.16.5",
    "cheerio": "1.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parser": "3.0.0",
    "csv-writer": "1.6.0",
    "dotenv": "17.2.3",
    "express": "^5.1.0",
    "graceful-fs": "4.2.11",
    "helmet": "^8.1.0",
    "https-proxy-agent": "7.0.6",
    "ioredis": "^5.8.1",
    "morgan": "^1.10.1",
    "node-fetch": "3.3.2",
    "node-html-parser": "^7.0.1",
    "nodemailer": "6.9.8",
    "puppeteer": "^24.31.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "redis": "^5.8.3",
    "uuid": "12.0.0"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  }
}
